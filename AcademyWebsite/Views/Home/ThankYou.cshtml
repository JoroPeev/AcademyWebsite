@model List<RegistrationData>

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <title>Thank You</title>
    <link href="https://stackpath.bootstrapcdn.com/bootstrap/5.3.0/css/bootstrap.min.css" rel="stylesheet">
</head>
<body>
    <div class="container text-center mt-5">
        <div class="card p-4 shadow-lg border-0">
            <h1 class="display-4">Thank You!</h1>
            <table>
    <thead>
        <tr>
            <th>
                ChildName
            </th>
            <th>
                ChildAge
            </th>
            <th>
                ParentName
            </th>
            <th>
                PhoneNumber
            </th>
            <th>
                EmailAddress
            </th>
            <th>
                Actions
            </th>
        </tr>
    </thead>
    <tbody>
        @{
        foreach (var item in Model)
        {
        <tr>
        <td>@item.ChildName</td>
        <td>@item.ChildAge</td>
        <td>@item.ParentName</td>
        <td>@item.PhoneNumber</td>
        <td>@item.EmailAddress</td>
        <td>

        <a asp-action="EditRegistrationData" asp-route-id="@item.Id">Edit</a>
        <a asp-action="EditRegistrationData" asp-route-id="@item.Id">Delete</a>
        
        </td>
        </tr>
        }
        }
    </tbody>
</table>
            <a asp-controller="Home" asp-action="Index" class="btn btn-primary">Return to Home</a>
        </div>
    </div>
    <script src="https://stackpath.bootstrapcdn.com/bootstrap/5.3.0/js/bootstrap.bundle.min.js"></script>
</body>
</html>
